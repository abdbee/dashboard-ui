{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": ["^(react)([-a-zA-Z]+)?$", "^(next)([-a-zA-Z/]+)?$", "^[./]", "^[-a-zA-Z/]+"],
  "importOrderSeparation": true
}
